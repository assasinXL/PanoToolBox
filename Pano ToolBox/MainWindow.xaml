<Window x:Class="Pano_ToolBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pano_ToolBox"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="177*"/>
            <ColumnDefinition Width="23*"/>
        </Grid.ColumnDefinitions>
        <Menu x:Name="menu" FontSize="18" Grid.ColumnSpan="2" Margin="0,0,0,390" HorizontalAlignment="Left" Width="600">
            <MenuItem Header="1" x:Name="a">
                <MenuItem Header="11" x:Name="a1" Click="Run_Click"/>
            </MenuItem>
        </Menu>
        <ListView x:Name="xlsxlist" Margin="350,30,10,150" Loaded="xlsxlist_Loaded" Grid.ColumnSpan="2"/>
        <RichTextBox x:Name="msgbox" Margin="10,266,10,10" IsReadOnly="True" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
